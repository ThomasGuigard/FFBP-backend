{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "5.3.1",
    "storageDir": "~/openapi-clients/ffbp-api-client",
    "generators": {
      "docs": {
        "generatorName": "typescript-angular",
        "output": "#{cwd}/output/v1.0/#{ext}/#{name}",
        "glob": "**/v1.0/*.{json}"
      }
    }
  }
}
